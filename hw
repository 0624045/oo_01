class CTriangle extends CShape
{
protected double a,b,c;
public CTriangle (double 3,double 4,double 5)
{
a=3;
b=4;
c=5;
}
public void show()
{
System.out.print("color="+color+",");
System.out.println("area"="0.5*a*b")
}
}
public class colorcolor
{
public static void main(String args[])
{
CTriangle rect=new CTriangle(5,10);
rect.setColor("Red");
rect.show();
}
}
